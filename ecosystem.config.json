{
  "apps": [
    {
      "name": "survey-app",
      "script": "npm",
      "args": "start",
      "cwd": "/path/to/your/project",
      "instances": "max",
      "exec_mode": "cluster",
      "env": {
        "NODE_ENV": "production",
        "PORT": 9886
      },
      "log_file": "./logs/app.log",
      "out_file": "./logs/out.log",
      "error_file": "./logs/error.log",
      "merge_logs": true,
      "time": true
    }
  ]
}